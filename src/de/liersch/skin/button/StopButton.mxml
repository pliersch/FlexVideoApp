<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark" xmlns:fx="http://ns.adobe.com/mxml/2009">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<fx:Script>
		<![CDATA[
			import de.liersch.preference.Colors;
		]]>
	</fx:Script>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:Rect height="12" width="12" x="5" y="6">
		<s:fill>
			<s:SolidColor color="#9BAC99"/>
		</s:fill>
	</s:Rect>
	<s:Rect height="22" width="22" includeIn="disabled,up" >
		<s:stroke>
			<s:SolidColorStroke caps="none" color="#9BAC99" joints="miter" miterLimit="10"/>
		</s:stroke>
		<s:fill>
			<s:SolidColor color="{Colors.ORANGE}" alpha="0"/>
		</s:fill>
	</s:Rect>
	
	<s:Rect height="22" width="22" includeIn="over,down" >
		<s:stroke>
			<s:SolidColorStroke caps="none" color="{Colors.ORANGE}" joints="miter" miterLimit="10"/>
		</s:stroke>
		<s:fill>
			<s:SolidColor color="{Colors.ORANGE}" alpha="0"/>
		</s:fill>
	</s:Rect>
</s:Skin>
